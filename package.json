{
  "name": "jquery-scroll-into-view",
  "version": "1.0.0",
  "main": "dist/jquery.scroll-into-view.js",
  "scripts": {
    "scripts": "rollup -c rollup.dev.js -w",
    "styles": "node-sass demo/assets/scss/styles.scss demo/assets/css/styles.css -w",
    "dev": "concurrently --kill-others \"npm run scripts\" \"npm run styles\"",
    "build": "rollup -c rollup.build.js",
    "demo": "node_modules/.bin/browser-sync start --config bs-config.js"
  },
  "files": [
    "*.js"
  ],
  "dependencies": {
    "jquery": ">=1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "babelrc-rollup": "^3.0.0",
    "concurrently": "^3.4.0",
    "node-sass": "^4.5.0",
    "rollup": "^0.37.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "uglify-js": "^2.8.13",
    "yargs": "^7.0.2"
  },
  "title": "jQuery Scroll Into View",
  "description": "A jquery plugin to scroll an element into view, with a few helpful features / options",
  "homepage": "https://github.com/davestewart/jquery-scroll-into-view",
  "author": {
    "name": "Dave Stewart",
    "url": "http://davestewart.io"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:davestewart/jquery-scroll-into-view.git"
  },
  "keywords": [
    "jquery",
    "jquery-plugin",
    "ecosystem:jquery",
    "ui",
    "scrollable"
  ],
  "bugs": "https://github.com/davestewart/jquery-scroll-into-view/issues",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/davestewart/jquery-scroll-into-view/LICENSE"
    }
  ],
  "license": "MIT"
}
